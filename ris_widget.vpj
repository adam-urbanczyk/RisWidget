			<F N="source/cpp/ViewWidget.cpp"/>
			<F N="source/cpp/ViewWidget.h"/>
		</Folder>
		<Folder
			Name="gpu"
			Filters="">
			<F N="source/gpu/histogram.cl"/>
			<F N="source/gpu/histogram.glslf"/>
			<F N="source/gpu/histogram.glslv"/>
			<F N="source/gpu/image.glslf"/>
			<F N="source/gpu/image.glslv"/>
		</Folder>
		<Folder
			Name="py"
			Filters="">
