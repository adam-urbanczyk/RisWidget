			<F N="source/cpp/ris_widget.sip"/>
			<F N="source/cpp/RisWidget.cpp"/>
			<F N="source/cpp/RisWidget.h"/>
			<F N="source/cpp/RisWidget.pro"/>
			<F N="source/cpp/RisWidget.qrc"/>
			<F N="source/cpp/RisWidget.ui"/>
			<F N="source/cpp/RisWidget.vcxproj"/>
