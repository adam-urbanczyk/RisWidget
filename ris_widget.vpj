			</Folder>
			<F N="ris_widget/__init__.py"/>
			<F N="ris_widget/_qt_debug.py"/>
			<F N="ris_widget/flipbook.py"/>
			<F N="ris_widget/ris_widget.py"/>
			<F N="ris_widget/shared_resources.py"/>
		</Folder>
		<F N="LICENSE"/>
		<F N="MANIFEST.in"/>
		<F N="README.md"/>
		<F N="setup.py"/>
	</Files>
</Project>
