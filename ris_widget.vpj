				Deletable="0"
				SaveOption="SaveNone">
				<Exec
					CmdLine="gnucoptions"
					Type="Slick-C"/>
			</Target>
		</Menu>
		<List Name="GNUC Options">
			<Item
				Name="LinkerOutputType"
				Value="Executable"/>
		</List>
		<Includes/>
		<Libs PreObjects="0"/>
	</Config>
	<CustomFolders></CustomFolders>
	<Files AutoFolders="DirectoryView">
		<Folder Name="ris_widget">
			<Folder Name="image">
				<Folder Name="ndimage_statistics">
					<F N="ris_widget/image/ndimage_statistics/__init__.py"/>
					<F N="ris_widget/image/ndimage_statistics/_ndimage_statistics.cpp"/>
					<F N="ris_widget/image/ndimage_statistics/_ndimage_statistics.pyx"/>
					<F N="ris_widget/image/ndimage_statistics/_ndimage_statistics_impl.cpp"/>
					<F N="ris_widget/image/ndimage_statistics/_ndimage_statistics_impl.h"/>
					<F N="ris_widget/image/ndimage_statistics/ndimage_statistics.py"/>
				</Folder>
				<F N="ris_widget/image/__init__.py"/>
